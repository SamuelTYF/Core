<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <StartupObject>Automata.IDE.Program</StartupObject>
    <Authors>Pumpkin</Authors>
    <Company>Pumpkin</Company>
    <ApplicationIcon />
